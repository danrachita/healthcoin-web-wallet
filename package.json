{
  "name": "web-wallet",
  "version": "1.0.0",
  "description": "Web Wallet for PoS Coins",
  "private": true,
  "node-main": "./app.js",
  "main": "loading.html",
  "keywords": [
    "coin, altcoin, wallet"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/onsightit/web-wallet.git"
  },
  "author": {
    "name": "Steve Woods",
    "email": "onsightit@gmail.com"
  },
  "readme": "See README.md\n",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/onsightit/web-wallet/issues"
  },
  "window": {
    "show": true,
    "title": "Web Wallet",
    "icon": "./public/wallet/images/Icon.png",
    "toolbar": false,
    "frame": true,
    "width": 1024,
    "height": 700,
    "position": "center",
    "min_width": 800,
    "min_height": 600
  },
  "dependencies": {
    "atob": "latest",
    "bcryptjs": "latest",
    "body-parser": "latest",
    "btoa": "latest",
    "coin-node": "1.0.0",
    "connect-flash": "~0.1.1",
    "cookie": "latest",
    "cookie-parser": "latest",
    "cryptoaddress-validator": "0.0.1",
    "ejs": "latest",
    "express": "latest",
    "express-session": "latest",
    "jsonminify": "0.4.1",
    "mongode": "0.1.6",
    "mongoose": "latest",
    "morgan": "~1.5.3",
    "passport": "~0.2.2",
    "passport-facebook": "~2.0.0",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "~1.0.0",
    "passport-oauth2": "~1.0.0",
    "passport-twitter": "^1.0.4",
    "request": "~2.75.0",
    "serve-favicon": "latest",
    "socket.io": "1.7.1",
    "uuid": "2.0.3",
    "validator": "~6.0.0",
    "xss": "0.1.7"
  }
}
